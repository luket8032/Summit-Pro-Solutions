FROM DischargeSummaryPatientInfo as a 
INNER JOIN DischargeSummaryAdmissionReason as b
ON a.ACN = b.ACN
INNER JOIN DischargeSummaryMedication as c
ON a.ACN = c.ACN
INNER JOIN SummaryPhysician as d
ON a.ACN = d.ACN
INNER JOIN DischargeSummaryDepression as e
ON a.ACN = e.ACN
INNER JOIN DischargeSummaryAnxiety as f
ON a.ACN = f.ACN
INNER JOIN DischargeSummaryTrauma as g
ON a.ACN = g.ACN
INNER JOIN DischargeSummaryMania as h
ON a.ACN = h.ACN
INNER JOIN DischargeSummarySafety as i
ON a.ACN = i.ACN
INNER JOIN DischargeSummaryDisruptivity as j
ON a.ACN = j.ACN
INNER JOIN DischargeSummaryAttention as k
ON a.ACN = k.ACN
INNER JOIN DischargeSummaryHPI as l
ON a.ACN = l.ACN
INNER JOIN DischargeSummaryMSERecommendations as m
ON a.ACN = m.ACN
INNER JOIN DischargeSummaryDischargeLocations as n
ON a.ACN = n.ACN
INNER JOIN DischargeSummaryDiagnosis as o
ON a.ACN = o.ACN
INNER JOIN DischargeSummaryStressors as p
ON a.ACN = p.ACN
INNER JOIN DischargeSummaryDischargePlan as q
ON a.ACN = q.ACN
WHERE a.ACN = 53239821780
